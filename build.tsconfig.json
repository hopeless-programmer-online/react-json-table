{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "outDir": "dist",
    "declaration": true
  },
  "include": [
    "src/table.tsx",
    "src/table.module.scss",
    "src/key-tree.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
